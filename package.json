{
  "name": "sovryn-onboard",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "prettier --write"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.3",
    "@sovryn/onboard-eslint-config": "*",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "typescript": "^4.5.2",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@changesets/cli": "^2.24.3"
  },
  "packageManager": "yarn@1.22.19"
}
