{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "importOrder": [
    "^react$",
    "^(?!.*app/)\\w",
    "^(@sovryn)(/.*|$)",
    "^app/",
    "^[./|~/]"
  ],
  "importOrderSeparation": true
}
